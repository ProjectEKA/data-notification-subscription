<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="202009291330" author="Neeraj" context="POC">
        <preConditions>
            <not>
                <tableExists tableName="hiu_subscription"/>
            </not>
        </preConditions>
        <comment>Add hiu_subscription table</comment>

        <createTable tableName="hiu_subscription">
            <column name="id" autoIncrement="true" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="request_id" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="patient_id" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="requester_type" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="details" type="JSON">
                <constraints nullable="false"/>
            </column>
            <column name="date_created" type="DATETIME" defaultValueDate="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="date_modified" type="DATETIME" defaultValueDate="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="202016090319" author="LeelaPrasanth" context="POC">
        <addColumn tableName="hiu_subscription">
            <column name="subscription_id" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="202009831330" author="LeelaPrasanth" context="POC">
        <preConditions>
            <not>
                <tableExists tableName="subscription_source"/>
            </not>
        </preConditions>
        <comment>Add sources table</comment>

        <createTable tableName="subscription_source">
            <column name="id" autoIncrement="true" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="subscription_id" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="category_link" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="category_data" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="hip_id" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="from" type="DATETIME" >
                <constraints nullable="false"/>
            </column>
            <column name="to" type="DATETIME" >
                <constraints nullable="false"/>
            </column>
            <column name="hiTypes" type="JSON">
                <constraints nullable="false"/>
            </column>
            <column name="date_created" type="DATETIME" defaultValueDate="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="date_modified" type="DATETIME" defaultValueDate="CURRENT_TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="202010141501" author="Dubey, LeelaPrasanth" context="POC">
        <renameColumn tableName="subscription_source" newColumnName="period_from" oldColumnName="from"/>
    </changeSet>
    <changeSet id="202010141502" author="Dubey, LeelaPrasanth" context="POC">
        <renameColumn tableName="subscription_source" newColumnName="period_to" oldColumnName="to"/>
    </changeSet>
    <changeSet id="202010141505" author="Dubey, LeelaPrasanth" context="POC">
        <renameColumn tableName="subscription_source" newColumnName="hi_types" oldColumnName="hiTypes"/>
    </changeSet>
    <changeSet id="202010161519" author="Dubey" context="Subscription">
        <addColumn tableName="subscription_source">
            <column name="status" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
    <changeSet id="202010251519" author="Dubey" context="Subscription">
        <addColumn tableName="subscription_source">
            <column name="excluded" type="BOOLEAN" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
    <changeSet id="202011031519" author="Dubey" context="Subscription">
        <addColumn tableName="subscription_source">
            <column name="excluded" type="BOOLEAN" defaultValueBoolean="false">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>